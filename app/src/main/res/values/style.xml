<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Main_text" parent="TextAppearance.AppCompat">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/twenty_five_sp</item>
    </style>

    <style name="subtext" parent="TextAppearance.AppCompat">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/twenty_sp</item>
    </style>

    <style name="toolbar" parent="">
        <item name="cornerRadius">@dimen/margin_20dp</item>
        <item name="backgroundTint">@color/blue_700</item>
    </style>

    <style name="Btn" parent="">
        <item name="backgroundTint">@color/white</item>
    </style>
    
    <style name="CustomTabStyle" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">@dimen/ten_sp</item>
        <item name="android:background">@color/blue_700</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:singleLine">true</item>
    </style>

</resources>